# The generic bit of the doc-install.pl command line.
@ENABLE_DOCUMENTATION_TRUE@doc_install_cmd = $(doc_install) --verbose --mode=0644

# Transform $(datarootdir) into a URI to match MM_ARG_WITH_TAGFILE_DOC().
@ENABLE_DOCUMENTATION_TRUE@datarootdir_esc = $(subst $(subst ,, ),%20,$(subst \,/,$(datarootdir)))
@ENABLE_DOCUMENTATION_TRUE@docdir_base_uri = file:///$(patsubst /%,%,$(datarootdir_esc:/=))/doc

# The command and options used to install the files from the HTML reference
# documentation.  The $(subst) magic translates external tag references from
# absolute to relative paths if the destination is on the local file system
# and installed under the same prefix as the package being built.
@ENABLE_DOCUMENTATION_TRUE@htmlref_relinst = $(subst @$(docdir_base_uri)/,@../../../,$(DOCINSTALL_FLAGS))
@ENABLE_DOCUMENTATION_TRUE@htmlref_install = $(doc_install_cmd) $(htmlref_relinst)

# The command and options used to install the Devhelp file.
@ENABLE_DOCUMENTATION_TRUE@devhelp_install = $(doc_install_cmd) --book-base='$(htmlrefdir:/=)'

# Helper variables to replicate each pattern with a $(srcdir)/ prefix.
# Also add quoting to prevent the shell from expanding the patterns.
@ENABLE_DOCUMENTATION_TRUE@htmlref_patterns_dup = $(foreach item,$(htmlref_patterns),'$(item)' '$(srcdir)/$(item)')
@ENABLE_DOCUMENTATION_TRUE@htmlref_patterns_quote = $(patsubst %,'%',$(htmlref_patterns))
@ENABLE_DOCUMENTATION_TRUE@htmlref_patterns_vpath = $(if $(srcdir:.=),$(htmlref_patterns_dup),$(htmlref_patterns_quote))

# Expand to a list of -name 'PATTERN' arguments for use with 'find'.
@ENABLE_DOCUMENTATION_TRUE@htmlref_find_patterns = $(patsubst %,-name '%' -o,$(notdir $(htmlref_patterns))) -false

# The parameters to the Doxygen-to-Devhelp XSLT script
@ENABLE_DOCUMENTATION_TRUE@dh_xsl_params = --stringparam book_title '$(book_title)' \
@ENABLE_DOCUMENTATION_TRUE@		--stringparam book_name '$(book_name)' \
@ENABLE_DOCUMENTATION_TRUE@		--stringparam book_base html


# Generated configuration files which, when updated, should cause the
# reference documentation to be rebuilt.
@ENABLE_DOCUMENTATION_TRUE@doc_config_deps = $(CONFIG_HEADER) $(srcdir)/$(doc_config).in $(srcdir)/Makefile.in
@ENABLE_DOCUMENTATION_TRUE@@MAINTAINER_MODE_FALSE@doc_dependencies = 

# Regenerate the documentation automatically only in maintainer mode.
# Depend on the generated configuration header files to trigger a rebuild
# if a configuration value changed.  The configuration header files only
# have their timestamp modified when the content actually changed, which
# is not the case for any other files generated by configure.
@ENABLE_DOCUMENTATION_TRUE@@MAINTAINER_MODE_TRUE@doc_dependencies = $(doc_config_deps) $(doc_input)
