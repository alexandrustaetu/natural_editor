# The name of the sub-directory where the generated documentation
# will be placed.
@ENABLE_DOCUMENTATION_TRUE@doc_outdir ?= reference

# The name of the Doxygen configuration file.
@ENABLE_DOCUMENTATION_TRUE@doc_config ?= $(doc_outdir)/Doxyfile

# The base URL where the online documentation for C++ binding modules
# is located, including the trailing slash.
@ENABLE_DOCUMENTATION_TRUE@pubdocbase ?= http://library.gnome.org/devel/

# The URL of the module's online HTML reference documentation, which
# may or may not end in a trailing slash.
@ENABLE_DOCUMENTATION_TRUE@htmlrefpub ?= $(pubdocbase)$(PACKAGE_TARNAME)/unstable/

# The title of the generated Devhelp book.
@ENABLE_DOCUMENTATION_TRUE@book_title ?= $(PACKAGE_NAME) Reference Manual

# A list of wildcard patterns matching the files from the HTML directory
# generated by Doxygen which should be distributed and installed.
@ENABLE_DOCUMENTATION_TRUE@htmlref_patterns ?= $(addprefix $(doc_outdir)/html/*.,css gif html png js)

# Locations of utilities shipped with glibmm.  Made overridable
# in case the installed utilities cannot be used for some reason.
@ENABLE_DOCUMENTATION_TRUE@doc_postprocess     ?= $(PERL) -- "$(MMDOCTOOLDIR)/doc-postprocess.pl"
@ENABLE_DOCUMENTATION_TRUE@doc_install         ?= $(PERL) -- "$(MMDOCTOOLDIR)/doc-install.pl"
@ENABLE_DOCUMENTATION_TRUE@tagfile_to_devhelp2 ?= "$(MMDOCTOOLDIR)/tagfile-to-devhelp2.xsl"

# Names of the main output files.
@ENABLE_DOCUMENTATION_TRUE@doxytagfile ?= $(doc_outdir)/$(book_name).tag
@ENABLE_DOCUMENTATION_TRUE@devhelpfile ?= $(doc_outdir)/$(book_name).devhelp2

# Explicitly depend on the files to be distributed or installed.
@ENABLE_DOCUMENTATION_TRUE@all-local: $(doc_inst_files) $(doc_dist_files)

# Hook up custom rules for translating references to external documentation
# to the actual location at install time.
@ENABLE_DOCUMENTATION_TRUE@install-data-local: $(doc_inst_targets)

# Hook up corresponding custom uninstall rules.
@ENABLE_DOCUMENTATION_TRUE@uninstall-local: $(addprefix un,$(doc_inst_targets))

# Install the HTML reference documentation files with just one invocation
# of doc-install.pl to speed up the build process.  Make use of the --glob
# option, which tells it to perform filename globbing itself, like 'find'.
# This helps to avoid excessively long command lines, as some platforms
# have rather restrictive limits.
@ENABLE_DOCUMENTATION_TRUE@install-htmlref: $(doc_outdir)/html/index.html
@ENABLE_DOCUMENTATION_TRUE@	@$(NORMAL_INSTALL)
@ENABLE_DOCUMENTATION_TRUE@	$(MKDIR_P) '$(DESTDIR)$(htmlrefdir)'
@ENABLE_DOCUMENTATION_TRUE@	$(htmlref_install) -t '$(DESTDIR)$(htmlrefdir)' --glob -- $(htmlref_patterns_vpath)

# Delete files from the html installation directory.  Avoid recursive
# directory removal, and apply the same wildcard pattern as was used to
# select files for installation.
@ENABLE_DOCUMENTATION_TRUE@uninstall-htmlref:
@ENABLE_DOCUMENTATION_TRUE@	@$(NORMAL_UNINSTALL)
@ENABLE_DOCUMENTATION_TRUE@	(cd '$(DESTDIR)$(htmlrefdir)' 2>/dev/null || exit 0; \
@ENABLE_DOCUMENTATION_TRUE@	 find . -type f '(' $(htmlref_find_patterns) ')' -exec rm -f '{}' '+')
@ENABLE_DOCUMENTATION_TRUE@	-test ! -r '$(DESTDIR)$(htmlrefdir)' || rmdir '$(DESTDIR)$(htmlrefdir)'

# Install the Devhelp file, translating the base path on the fly.
@ENABLE_DOCUMENTATION_TRUE@install-devhelp: $(devhelpfile)
@ENABLE_DOCUMENTATION_TRUE@	@$(NORMAL_INSTALL)
@ENABLE_DOCUMENTATION_TRUE@	$(MKDIR_P) '$(DESTDIR)$(devhelpdir)'
@ENABLE_DOCUMENTATION_TRUE@	$(devhelp_install) -t '$(DESTDIR)$(devhelpdir)' -- $^

# Remove the installed Devhelp file and directory.
@ENABLE_DOCUMENTATION_TRUE@uninstall-devhelp:
@ENABLE_DOCUMENTATION_TRUE@	@$(NORMAL_UNINSTALL)
@ENABLE_DOCUMENTATION_TRUE@	rm -f '$(DESTDIR)$(devhelpdir)/$(notdir $(devhelpfile))'
@ENABLE_DOCUMENTATION_TRUE@	-test ! -r '$(DESTDIR)$(devhelpdir)' || rmdir '$(DESTDIR)$(devhelpdir)'

# Regenerate the Doxygen configuration file automatically.  In the
# top-level build directory Automake already takes care of this.
@ENABLE_DOCUMENTATION_TRUE@ ifneq ($(subdir),.)
@ENABLE_DOCUMENTATION_TRUE@$(doc_config): $(srcdir)/$(doc_config).in $(top_builddir)/config.status
@ENABLE_DOCUMENTATION_TRUE@	$(AM_V_GEN)cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@

@ENABLE_DOCUMENTATION_TRUE@ endif
# Make sure that the documentation will always have been generated before
# executing commands of a rule that depends on files in $(doc_outdir)/html/.
@ENABLE_DOCUMENTATION_TRUE@$(doc_outdir)/html/%: | $(doxytagfile)

# Run Doxygen to build the reference documentation. The generated tag file
# also functions as time stamp target for the documentation as a whole.
# Quote $(DOXYGEN) so that this still works if this is a path containing
# spaces such as "/c/Program Files (x86)/Doxygen/bin/doxygen"
# The doc_config file may contain "$(MMDOCTOOLDIR)". Export MMDOCTOOLDIR to Doxygen.
@ENABLE_DOCUMENTATION_TRUE@$(doxytagfile): $(doc_dependencies) | $(doc_config)
@ENABLE_DOCUMENTATION_TRUE@	-$(AM_V_at)rm -f $@
@ENABLE_DOCUMENTATION_TRUE@	-$(AM_V_at)rm -fr $(doc_outdir)/html
@ENABLE_DOCUMENTATION_TRUE@	$(AM_V_GEN)(echo '@INCLUDE =' $(doc_config) && echo 'INPUT =' $(doc_input)) | MMDOCTOOLDIR="$(MMDOCTOOLDIR)" "$(DOXYGEN)" -
@ENABLE_DOCUMENTATION_TRUE@	$(AM_V_at)$(doc_postprocess) '$(doc_outdir)/html/*.html'

# Run XSL transformation to generate a Devhelp book from a Doxygen tag file.
@ENABLE_DOCUMENTATION_TRUE@%.devhelp2: %.tag
@ENABLE_DOCUMENTATION_TRUE@	$(AM_V_GEN)$(XSLTPROC) $(dh_xsl_params) -o $@ $(tagfile_to_devhelp2) $<

@ENABLE_DOCUMENTATION_TRUE@.PHONY: install-htmlref uninstall-htmlref install-devhelp uninstall-devhelp

# Instruct GNU make to delete the targets of a rule after it failed, in
# order to avoid the complication of handling that situation manually.
@ENABLE_DOCUMENTATION_TRUE@.DELETE_ON_ERROR:

@ENABLE_DOCUMENTATION_TRUE@docs/manual/html/index.html: docs/manual/libxml++.xml docs/manual/docbook-customisation.xsl
